{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "@gv/*": ["./packages/*/src"]
    }
  },
  "exclude": [
    "apps/web/app/robots.txt",
    "apps/web/app/sitemap.xml"
  ]
}