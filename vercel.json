{
  "crons": [],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff2?|ttf|ico|svg|png|jpg|webp|avif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(about|pricing|contact|roadmap|integrations|status|trust|get-started|media-kit|changelog|policies|products|blog)(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}

